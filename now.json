{
  "name": "nextjs-with-firebase",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    { "src": "/static/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "version": 2
}
